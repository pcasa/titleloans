<%= div_for(photo, :class => "photo") do %>
	<%= link_to image_tag(photo.image_url(:thumb)), photo.image_url(:large), :class => "img", :rel => "photo_group" if photo.image? %>
	<%= link_to "Delete", [current_company, @title_loan, photo], :method => :delete, :confirm => "Are you sure?", :remote => :true %>
<% end %>